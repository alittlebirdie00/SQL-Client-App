<AnchorPane prefHeight="595.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SQLClient.SQLClientController">

    <children>
        <ComboBox fx:id="JDBCDriverCombobox" layoutX="115.0" layoutY="50.0" prefHeight="27.0" prefWidth="230.0" />
