        <Button fx:id="clearResultWindowButton" layoutX="41.0" layoutY="536.0" mnemonicParsing="false" onAction="#clearResultWindowButtonPressed" text="Clear Result Window" AnchorPane.leftAnchor="20.0" />
        <Label layoutX="15.0" layoutY="55.0" text="JDBC Driver" AnchorPane.leftAnchor="20.0" />
        <Label layoutX="15.0" layoutY="105.0" text="Database URL" AnchorPane.leftAnchor="20.0" />
        <Label layoutX="21.0" layoutY="156.0" text="Username" AnchorPane.leftAnchor="21.0" />
        <Label layoutX="20.0" layoutY="212.0" text="Password" AnchorPane.leftAnchor="20.0" />
        <Label layoutX="326.0" layoutY="14.0" text="Enter An SQL Command" textFill="#d69402" AnchorPane.rightAnchor="20.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Label>
        <Label layoutX="18.0" layoutY="14.0" text="Enter Database Information" textFill="#027dd6" AnchorPane.leftAnchor="20.0" />
        <ImageView fitHeight="53.0" fitWidth="101.0" layoutX="611.0" layoutY="526.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="20.0">
            <image>
                <Image url="@logo-mysql.png" />
            </image>
        </ImageView>
      <TableView fx:id="resultTableView" layoutX="19.0" layoutY="319.0" prefHeight="200.0" prefWidth="779.0" />
    </children>

</AnchorPane>
