        <TextField fx:id="usernameTextField" layoutX="115.0" layoutY="151.0" prefHeight="27.0" prefWidth="230.0" />
        <TextField fx:id="passwordTextField" layoutX="116.0" layoutY="204.0" prefHeight="27.0" prefWidth="230.0" />
        <TextArea fx:id="sqlCommandTextArea" layoutX="326.0" layoutY="48.0" prefHeight="221.0" prefWidth="425.0" AnchorPane.rightAnchor="20.0" />
        <Button fx:id="connectToDatabaseButton" layoutX="274.0" layoutY="285.0" mnemonicParsing="false" onAction="#connectToDatabaseButtonPressed" prefHeight="27.0" prefWidth="150.0" text="Connect to Database" AnchorPane.leftAnchor="274.0" />
        <Button fx:id="clearSQLCommandButton" layoutX="450.0" layoutY="284.0" mnemonicParsing="false" onAction="#clearSQLCommandButtonPressed" prefWidth="150.0" text="Clear SQL Command" AnchorPane.rightAnchor="216.0" />
        <Button fx:id="executeSQLCommandButton" layoutX="529.0" layoutY="283.0" mnemonicParsing="false" onAction="#executeSQLCommandButtonPressed" prefWidth="170.0" text="Execute SQL Command" AnchorPane.rightAnchor="20.0" />
